{
	"$schema": "https://raw.githubusercontent.com/liferay/liferay-js-toolkit/master/resources/schemas/configuration.schema.json",
	"system": {
		"category": "others",
		"name": "angular-pitcher",
		"fields": {
			"fruit": {
				"type": "string",
				"name": "fruit",
				"description": "fruit-help",
				"default": "orange",
				"options": {
					"orange": "an-orange",
					"pear": "a-pear",
					"apple": "an-apple"
				}
			}
		}
	},
	"portletInstance": {
		"fields": {
			"page": {
				"type": "string",
				"name": "second-page-to-send-ball",
				"description": "relative-path",
				"default": "/web/guest/secondpage"
			}
		}
	}
}